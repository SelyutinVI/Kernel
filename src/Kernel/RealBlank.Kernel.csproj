<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Title>RealBlank.Kernel</Title>
    <PackageId>RealBlank.Kernel</PackageId>
    <AssemblyName>RealBlank.Kernel</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Authors>RealBlank (https://github.com/SelyutinVI)</Authors>
    <RepositoryUrl>https://github.com/SelyutinVI/RealBlank.Kernel</RepositoryUrl>
    <Copyright>Copyright Selyutin Vladislav</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Version>0.2.0-alpha</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MediatR" Version="12.4.1" />
  </ItemGroup>
  
  <ItemGroup>
    <Content Include="..\..\LICENSE" />
  </ItemGroup>

</Project>
